nents\\common\\Banner\\SwitchArrow\\index.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\components\\common\\Banner\\SwitchDot\\index.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\OldContext.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\NewContext.js",[],["207","208"],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\components\\common\\Form\\Test.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\components\\common\\Form\\index.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\components\\common\\Form\\formContext.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\components\\common\\Form\\FormInput.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\components\\common\\Form\\FormButton.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\utils\\helper.js",[],["209","210"],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\components\\common\\ErrorBound\\index.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\components\\StudentContainer\\index.js",["211"],"import React,{useState,useEffect} from 'react'\r\nimport StudentList from '../StudentList/index'\r\nimport Pager from '../common/Pager'\r\nimport { getStudents} from '../../services/student'\r\n\r\n/**\r\n * 分页组件\r\n * 属性\r\n * 1、current：初始页码\r\n * 2、total：总数据量\r\n * 3、limit：页容量，每页显示的数据量\r\n * 4、panelNumber:数字页码最多显示多少个\r\n * \r\n */\r\n\r\n/**\r\n * 用于提供数据，以及控制数据的变化\r\n */\r\nexport default function StudentContainer() {\r\n    const [students,setStudents] = useState([]);\r\n    const [panelNumber, setPanelNumber] = useState(5);\r\n    const [current, setCurrent] = useState(1);\r\n    const [total, setTotal] = useState(0);\r\n    const [limit, setLimit] = useState(10)\r\n    useEffect(() => {\r\n        (async function() {\r\n            if(current == '') {\r\n                return\r\n            }\r\n            const resp = await getStudents(current,limit);\r\n            \r\n            setTotal(resp.cont)\r\n            setStudents(resp.findByPage);\r\n            \r\n        })()\r\n        \r\n    }, [current,limit])//空数组的目的，是让该副作用函数没有任何依赖项，从而仅在首次挂载时运行\r\n    return (\r\n        <div>\r\n            <StudentList stus={students}/>\r\n            <div>\r\n            <Pager \r\n                panelNumber={panelNumber} \r\n                current={current} \r\n                limit={limit} \r\n                total={total}\r\n                onPageChange={newPage => setCurrent(newPage)}\r\n                />\r\n            </div>\r\n            <div>\r\n                最多显示的数字页码：<input type=\"number\" value={panelNumber} onChange={(e) => {\r\n                    setPanelNumber(e.target.value)\r\n                }}/>\r\n            </div>\r\n            <div>\r\n                每页显示的条数：<input type=\"number\" value={limit} onChange={(e) => {\r\n                    setLimit(e.target.value)\r\n                }}/>\r\n            </div>\r\n            \r\n            \r\n            {/* <input type=\"number\" value={current} onChange={(e) => {\r\n                setCurrent(e.target.value)\r\n            }}/> */}\r\n        </div>\r\n    )\r\n}\r\n",["212","213"],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\components\\StudentList\\index.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\components\\common\\Pager\\index.js",["214","215"],"import React from 'react'\r\nimport './index.css'\r\n\r\n/**\r\n * 分页组件\r\n * 属性\r\n * 1、current：初始页码\r\n * 2、total：总数据量\r\n * 3、limit：页容量，每页显示的数据量\r\n * 4、panelNumber:数字页码最多显示多少个\r\n * \r\n */\r\n\r\nexport default function Pager(props) {\r\n    const pageNumber = getPageNumber(props);\r\n    if(pageNumber == '0') {\r\n        return null;\r\n    }\r\n    console.log(props);\r\n    const min = getMinNumber(props);//获取最小数字\r\n    const max = getMaxNumber(min,pageNumber,props);//最大数字\r\n    const number = [];\r\n    for (let i = min; i<= max; i++) {\r\n        number.push(<span key={i} className=\"item\" onClick={() => {toPage(i,props)}} className={i===props.current ? \"item active\" : \"item\"}>{i}</span>)\r\n    }\r\n    return (\r\n        <>\r\n            <span \r\n                className={props.current === 1 ? \"item disabled\" : 'item'}\r\n                onClick={() => toPage(1,props)}\r\n            >首页</span>\r\n            <span\r\n                onClick={() => toPage(props.current -1 < 1 ? 1 : props.current - 1,props)} \r\n                className={props.current === 1 ? \"item disabled\" : 'item'}>上一页</span>\r\n            {/* 数字页码 */}\r\n            {number}\r\n            <span\r\n                onClick={() => {toPage(props.current + 1 > pageNumber ? pageNumber : props.current + 1,props)}}\r\n                 className={props.current === pageNumber ? \"item disabled\" : 'item'}>下一页</span>\r\n            <span\r\n                onClick={() => {toPage(pageNumber,props)}} \r\n                className={props.current === pageNumber ? \"item disabled\" : 'item'}>尾页</span>\r\n            <span className=\"current\">{props.current}</span>/\r\n            <span>{pageNumber}</span>\r\n        </>\r\n    )\r\n}\r\n\r\n\r\nfunction getMinNumber(props) {\r\n    var min = props.current - Math.floor(props.panelNumber / 2)\r\n    if(min < 1) {\r\n        min = 1;\r\n    }\r\n    return min;\r\n}\r\n\r\n/**\r\n * 计算最大数字\r\n * @param {*} min \r\n * @param {*} pageNumber \r\n * @param {*} props \r\n */\r\n\r\nfunction getMaxNumber(min,pageNumber,props){\r\n    var max = min + props.panelNumber -1 ;\r\n    if(max > pageNumber) {\r\n        max = pageNumber;\r\n    }\r\n    return max;\r\n}\r\n\r\n/**\r\n * 跳转到某一页\r\n * @param {*} target 目标页面\r\n * @param {*} props 所有属性\r\n */\r\n\r\nfunction toPage(target,props){\r\n    if(props.current === target) {\r\n        return; //目标和当前页面相同\r\n    }\r\n    props.onPageChange && props.onPageChange(target);\r\n}\r\n\r\n/**\r\n * 计算总页数\r\n */\r\nfunction getPageNumber(props) {\r\n    return Math.ceil(props.total / props.limit);\r\n}","C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\myHooks\\useAllStudents.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\myHooks\\usePageStudent.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\myHooks\\useTimer.js",["216","217"],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\useReducer.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\components\\common\\FadeTransition\\index.js",[],["218","219"],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\components\\Layout\\index.js",[],["220","221"],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\pages\\Login\\index.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\pages\\Admin\\index.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\components\\Header\\index.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\components\\Menu\\index.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\pages\\Welcome\\index.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\pages\\student\\StudentAdd.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\pages\\student\\StudentList.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\pages\\courses\\CourseAdd.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\pages\\courses\\CourseList.js",[],"C:\\personal\\学习\\react学习1\\reactLearn-fuxi\\react-learn\\src\\components\\StudentSearchBar\\index.js",[],{"ruleId":"222","severity":1,"message":"223","line":3,"column":1,"nodeType":"224","endLine":15,"endColumn":2},{"ruleId":"225","replacedBy":"226"},{"ruleId":"227","replacedBy":"228"},{"ruleId":"225","replacedBy":"229"},{"ruleId":"227","replacedBy":"230"},{"ruleId":"225","replacedBy":"231"},{"ruleI